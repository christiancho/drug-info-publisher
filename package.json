{
  "name": "prescriberpoint",
  "private": true,
  "packageManager": "yarn@4.9.2",
  "workspaces": [
    "apps/*",
    "functions",
    "types"
  ],
  "dependencies": {
    "@anthropic-ai/sdk": "^0.57.0",
    "@google/generative-ai": "^0.24.1",
    "@types/glob": "^9.0.0",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^24.1.0",
    "@types/pg": "^8.15.5",
    "dotenv": "^17.2.1",
    "glob": "^11.0.3",
    "himalaya": "^1.1.1",
    "is-html": "^3.1.0",
    "pg": "^8.16.3",
    "reflect-metadata": "^0.2.2",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.3",
    "typeorm": "^0.3.25",
    "typescript": "^5.8.3"
  },
  "devDependencies": {
    "@vitest/ui": "^1.0.0",
    "vitest": "^1.0.0"
  },
  "scripts": {
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage"
  },
  "vitest": {
    "environment": "node",
    "include": [
      "**/*.test.ts"
    ],
    "coverage": {
      "include": [
        "scripts/**/*.ts",
        "functions/src/**/*.ts"
      ],
      "exclude": [
        "**/*.d.ts",
        "**/*.test.ts"
      ]
    }
  }
}
